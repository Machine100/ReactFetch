{
  "StartAt": "Init",
  "States": {
    "Init": {
      "Type": "Pass",
      "Result": {"isfed": true},
      "Next": "check switchboard"
    },
	  "checkswitchboard ": {
  	  "Comment": "Look for input from user",
      "Type": "Task",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "TimeoutSeconds": 300,
      "HeartbeatSeconds": 60,
      "next": "checkcats"
	  },
    "checkcats": {
      "Comment": "See what cats have shown up",
      "Type": "task",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Result": {"isfed": true},
      "Next": "drawcats"
    },
    "drawcats": {
      "Comment": "Generate an image of the world",
      "Type": "task",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Result": {"isfed": true},
      "Next": "delay"
    },
    "delay": {
      "Comment": "Delay a bit",
      "Type": "wait",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Delay": {"isfed": true},
      "Next": "delaytimer"
    },
    "cleanup": {
      "Comment": "Clear out food bowls and let cats leave",
      "Type": "task",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Result": {"isfed": true},
      "Next": "checkswitchboard"
    },
  }
} 
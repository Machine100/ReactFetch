{
  "StartAt": "Init",
  "States": {
    "Init": {
      "Type": "Pass",
      "Result": {
                "food1": true, 
                "food2": true, 
                "toy1": true,
                "toy2": true,
                "toy3": false,
                "cat1": true,
                "cat2": false,
                "cat3": true,
                "cat4": false,
                "cat5": true,
                },
      "Next": "check switchboard"
    },
	  "checkswitchboard ": {
  	  "Comment": "Look for input from user. Disabled for now",
      "Type": "pass",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "TimeoutSeconds": 300,
      "HeartbeatSeconds": 60,
      "next": "checkcats"
	  },
    "checkcats": {
      "Comment": "See what cats have shown up",
      "Type": "task",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Result": {"isfed": true},
      "Next": "drawcats"
    },
    "drawcats": {
      "Comment": "Generate an image of the world",
      "Type": "task",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Result": {"isfed": true},
      "Next": "delay"
    },
    "delay": {
      "Comment": "Delay a bit",
      "Type": "wait",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Delay": {"isfed": true},
      "Next": "delaytimer"
    },
    "cleanup": {
      "Comment": "Clear out food bowls and let cats leave",
      "Type": "task",
      "Resource": "arn:aws:lambda:us-east-1:980965586942:function:responder",
      "Result": {"isfed": true},
      "Next": "checkswitchboard"
    },
  }
} 